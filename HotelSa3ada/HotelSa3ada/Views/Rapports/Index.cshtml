@model IEnumerable<HotelSa3ada.Models.Rapport>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateGeneration)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contenu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUtilisateurNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateGeneration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contenu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUtilisateurNavigation.IdUtilisateur)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdRapport">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdRapport">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdRapport">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
